{
  "name": "vscode-saturnus-syntax",
  "displayName": "vscode-saturnus-syntax",
  "description": "Syntax highlighting for Saturnus.",
  "version": "0.2.0",
  "publisher": "Argochamber",
  "repository": {
    "type": "git",
    "url": "https://github.com/sigmasoldi3r/vscode-saturnus-syntax.git"
  },
  "bugs": {
    "url": "https://github.com/dunstsigmasoldi3rontc/vscode-saturnus-syntax/issues"
  },
  "scripts": {
    "start": "gulp watch",
    "build": "gulp",
    "package": "vsce package",
    "publish": "vsce publish",
    "prepublish": "yarn package"
  },
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "saturnus",
        "aliases": [
          "saturnus"
        ],
        "extensions": [
          ".saturn"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "saturnus",
        "scopeName": "source.saturnus",
        "path": "./syntaxes/saturnus.tmLanguage.json"
      }
    ]
  },
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "icon": "assets/rust.png",
  "devDependencies": {
    "@vscode/vsce": "^2.19.0",
    "gulp": "^3.9.1",
    "gulp-json5-to-json": "^0.2.1",
    "gulp-merge-json": "^1.3.0"
  },
  "dependencies": {}
}
